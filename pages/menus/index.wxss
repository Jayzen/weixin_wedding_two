page{
  padding:30rpx;
  box-sizing: border-box;
}
/* 展示图 */
.content_img{
  width:100%;
  height:350rpx;
}
/* 标题 */
.photographs_name{
  font-size:18px;
  margin-top:20rpx;
}
/* 底部价格 */
.price{
  display:flex;
  align-items:center;
  margin:20rpx 0 30rpx 0;
  font-size:12px;
  color:#A1624F;
}
.price .price_num{
  font-size:16px;
  margin:0 20rpx 0 10rpx;
}
.price .more{
  color:#C1C2C3;
}



